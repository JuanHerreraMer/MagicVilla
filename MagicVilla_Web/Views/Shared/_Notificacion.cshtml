
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

@if (TempData["exitoso"] != null)
{
    <script>
        Swal.fire({
            positon: 'top-end',
            icon: 'success',
            title: '@TempData["exitoso"]',
            showConfirmButton: false,
            TimeRanges: 2500
        });
    </script>
}

@if (TempData["error"] != null)
{
    <script>
        Swal.fire({
            positon: 'top-end',
            icon: 'error',
            title: '@TempData["error"]',
            showConfirmButton: false,
            TimeRanges: 2500
        });
    </script>
}
